{
  "version": 2,
  "projects": {
    "tube-api": "apps/tube-api",
    "tube-csr": "apps/tube-csr",
    "tube-csr-e2e": "apps/tube-csr-e2e",
    "tube-ssr": "apps/tube-ssr",
    "tube-ssr-e2e": "apps/tube-ssr-e2e"
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "tube-csr"
}
